{
  "name": "protocol-sync",
  "version": "0.1.0",
  "description": "Keep your Protocol CSS modules in sync!",
  "keywords": [
    "protocol",
    "mozilla-protocol"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "bin": {
    "protocol-sync": "cli.js"
  },
  "scripts": {
    "pretest": "rm -rf __test__; ./cli.js __test__",
    "test": "node-sass --include-path=__test__ __test__/protocol/index.scss > __test__/protocol.css",
    "posttest": "du -h __test__/protocol.css"
  },
  "author": "Alexander Salas Bastidas (a.salas@ieee.org)",
  "license": "MIT",
  "dependencies": {
    "cosmiconfig": "^5.0.7",
    "fs-extra": "^7.0.1",
    "globby": "^9.0.0",
    "resolve": "^1.9.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "node-sass": "^4.11.0"
  }
}
